{
  "ServiceType": "${{ServiceType}}",
  "ConnectionStrings": {
    "SqlServer": "${{ConnectionStrings__SqlServer}}"
  },
  "Swagger": {
    "ApiEndpoint": "/swagger/v1/swagger.json",
    "ApiVersion": "v1"
  },
  "Firebase": {
    "ApiKey": "${{Firebase__ApiKey}}",
    "AuthApiBaseUrl": "${{Firebase__AuthApiBaseUrl}}",
    "AuthApiRequestUrl": "${{Firebase__AuthApiRequestUrl}}",
    "AuthorityUrl": "${{Firebase__AuthorityUrl}}",
    "ProjectId": "${{Firebase__ProjectId}}"
  },
  "AllowedHosts": "*"
}
